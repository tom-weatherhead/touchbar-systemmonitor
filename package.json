{
	"name": "touchbar-systemmonitor",
	"version": "0.0.0",
	"description": "A system monitor in Apple MacBook Pro touch bar -- use it with commmand: CommandOrControl+Shift+1",
	"private": false,
	"homepage": "https://github.com/tom-weatherhead/touchbar-systemmonitor#readme",
	"repository": "github:tom-weatherhead/touchbar-systemmonitor",
	"bugs": {
		"url": "https://github.com/tom-weatherhead/touchbar-systemmonitor/issues"
	},
	"main": "src/main.js",
	"scripts": {
		"start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder"
	},
	"build": {
		"appId": "Touch Bar System Monitor",
		"mac": {
			"target": "dmg",
			"icon": "src/icons/icon.icns"
		}
	},
	"dependencies": {
		"electron": "^22.0.0",
		"osx-temperature-sensor": "^1.0.8",
		"systeminformation": "^5.16.3"
	},
	"devDependencies": {
		"@electron/rebuild": "^3.2.9"
	},
	"engines": {
		"node": ">= 18"
	},
	"os": [
		"darwin"
	],
	"cpu": [
		"arm",
		"x64"
	],
	"author": "Tom Weatherhead <thaw@2hrd4u.org> (https://github.com/tom-weatherhead)",
